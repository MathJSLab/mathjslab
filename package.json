{
  "name": "mathjslab",
  "version": "1.6.0",
  "description": "MathJSLab - An interpreter with language syntax like MATLABÂ®/Octave, ISBN 978-65-00-82338-7.",
  "main": "lib/mathjslab.js",
  "types": "lib/src/lib.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathJSLab/mathjslab.git"
  },
  "keywords": [
    "MathJSLab",
    "Mathematics",
    "Educational",
    "MATLAB",
    "Octave",
    "MathML",
    "ANTLR",
    "ANTLR4",
    "math",
    "lexer",
    "parser",
    "grammar",
    "interpreter"
  ],
  "author": "Sergio Lindau <sergiolindau@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MathJSLab/mathjslab/issues"
  },
  "homepage": "https://mathjslab.com/",
  "scripts": {
    "clean:package-lock": "node script/clean-package-lock.cjs",
    "clean:resources": "rimraf res",
    "clean:build": "rimraf lib",
    "clean:parser-tokens-interp": "rimraf ./src/.antlr ./src/MathJSLabLexer.tokens ./src/MathJSLabLexer.interp ./src/MathJSLabParser.tokens ./src/MathJSLabParser.interp",
    "clean:parser-src": "rimraf ./src/MathJSLabLexer.ts ./src/MathJSLabParser.ts",
    "clean:parser": "npm run clean:parser-src && npm run clean:parser-tokens-interp",
    "clean": "npm run clean:build && npm run clean:parser",
    "clean:all": "npm run clean && npm run clean:resources && npm run clean:package-lock",
    "update": "npx ncu -u && npm install && npm run build:resources",
    "git:add": "git add .",
    "git:commit": "git commit -m \"Initial Commit\"",
    "git:push": "git push",
    "git:add:commit:push": "npm run git:add && npm run git:commit && npm run git:push",
    "npm:publish": "npm publish --access public",
    "add:commit:push:publish": "npm run git:add:commit:push && npm run npm:publish",
    "format": "prettier --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint \"**/*.ts{,x}\" --fix",
    "format:lint": "npm run format && npm run lint",
    "pretest": "npm run build:parser",
    "test": "jest --verbose",
    "pretest:watch": "npm run build:parser",
    "test:watch": "jest --watch",
    "tsx": "tsx --require tsconfig-paths/register",
    "build:resources": "npm run tsx script/build-resources.ts",
    "build:parser": "set CLASSPATH=./res/antlr-complete.jar && java org.antlr.v4.Tool ./src/MathJSLabLexer.g4 ./src/MathJSLabParser.g4 -Dlanguage=TypeScript -no-visitor -no-listener -o ./src/",
    "prebuild": "npm run build:parser",
    "build": "tsx --require tsconfig-paths/register ./node_modules/webpack/bin/webpack --mode production",
    "do:all": "npm run clean:all && npm run update && npm run format:lint && npm run test && npm run build && npm run add:commit:push:publish"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "29.5.13",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.2",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "29.7.0",
    "lint-staged": "^15.2.10",
    "node-html-parser": "^6.1.13",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "antlr4": "^4.13.2",
    "decimal.js": "^10.4.3"
  }
}
